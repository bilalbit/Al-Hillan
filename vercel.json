{
  "version": 2,
  "builds": [
    {
      "src": "src/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "src/pages/api/**",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/pages/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/next.config.js"
    }
  ]
}